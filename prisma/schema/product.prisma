model Product {
  id          Int      @id @default(autoincrement())
  title       String
  slug        String
  price       Int
  description String?
  images      String[]

  categoryId Int?
  category   Category? @relation(fields: [categoryId], references: [id])

  createdAt   DateTime      @default(now())
  ProductItem ProductItem[]

  @@map("products")
}
